{
  "version": 2,
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next@canary"
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "18.x",
      "NEXT_FORCE_CLEAN_BUILD": "true"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
} 